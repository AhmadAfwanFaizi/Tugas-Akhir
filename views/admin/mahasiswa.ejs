<script>
    function autoNumber() {
        var type = "POST";
        var url = "/client/getAutoNumber";
        $.ajax({
            type: type,
            url: url,
            dataType: "json",
            success: function (res) {
                var newid = "";
                if (res.data.length == 0) {
                    newid = "MHS001"
                } else {
                    var string = "MHS"
                    var array = []
                    res.data.forEach(id => {
                        var kode = id.idMahasiswa.substr(3, 6)
                        array.push(kode)
                    });
                    var maxx = Math.max.apply(null, array);
                    var mmax = maxx + 1
                    if (mmax >= 10 && mmax < 100) {
                        var neew = '' + mmax;
                        newid = string + "0" + neew
                    } else if (mmax < 10) {
                        var neew = '' + mmax;
                        newid = string + "00" + neew
                    } else {
                        newid = string + mmax;
                    }
                }
                $("#v_kode").val(newid)
            }
        })
    }

    var dataTablesMhs = {
        init: function () {
            $(document).ready(function () {
                $('#datatables').DataTable({
                    processing: true,
                    // "serverSide": true,
                    ajax: {
                        url: "/Mmahasiswa",
                        dataType: "json",
                        type: "POST"
                    },
                    columns: [
                        // {data: "idMahasiswa"},
                        {
                            data: "namaLengkap"
                        },
                        {
                            data: "jenisKelamin"
                        },
                        {
                            data: "agama"
                        },
                        {
                            data: "telepon"
                        },
                        {
                            data: "namaSekolah"
                        },
                        {
                            data: "id",
                            render: function (id) {
                                var but =
                                    '<button type="button" class="btn btn-success" data-toggle="modal" data-target="#edit_modal" onclick="edit(' +
                                    id +
                                    ')">Edit</button> <button type="button" class="btn btn-danger" onclick="delte(' +
                                    id + ')">Delete</button>';
                                return but;
                            },
                            width: '115px'
                        },
                    ]
                });
            });
        }
    }

    $(document).ready(function () {
        $("#add_button").click(function () {
            var type = "POST";
            var url = "/Mmahasiswa/save";
            var data = $("#add_form").serialize();
            $.ajax({
                type: type,
                url: url,
                dataType: "json",
                data: data,
                success: function (res) {
                    if (res.code === 200) {
                        swal({
                            title: "Success!",
                            text: res.message,
                            icon: "success",
                            button: "Close!",
                        }).then(function () {
                            $('#add_form')[0].reset();
                            $("#add_modal").modal('hide');
                            $('#datatables').DataTable().ajax.reload();
                        })
                    } else {
                        swal({
                            title: "Server Error!",
                            text: res.message,
                            icon: "error",
                            button: "Close!",
                        });
                    }
                }
            });
        });
    });

    $("#edit_modal").ready(function () {
        $("#edit_button").click(function () {
            swal({
                title: "Are you sure?",
                text: "",
                icon: "warning",
                buttons: true,
                dangerMode: true,
            }).then(function (e) {
                if (e) {
                    var type = "POST";
                    var url = "/Mmahasiswa/update";
                    var data = $("#edit_form").serialize();
                    $.ajax({
                        type: type,
                        url: url,
                        dataType: "json",
                        data: data,
                        success: function (res) {
                            if (res.code == 200) {
                                swal({
                                    title: "Success!",
                                    text: res.message,
                                    icon: "success",
                                    button: "Close!",
                                }).then(function () {
                                    $('#edit_form')[0].reset();
                                    $("#edit_modal").modal('hide');
                                    $('#datatables').DataTable().ajax
                                        .reload();
                                });
                            } else {
                                swal({
                                    title: "Server Error!",
                                    text: res.message,
                                    icon: "error",
                                    button: "Close!",
                                });
                            }
                        }
                    });
                } else {
                    swal({
                        title: "Success!",
                        text: "Data Gagal Diubah!",
                        icon: "success",
                        button: "Close!",
                    }).then(function () {
                        $('#edit_form')[0].reset();
                        $("#edit_modal").modal('hide');
                        $('#datatables').DataTable().ajax.reload();
                    });
                }
            })
        });
    });



    function edit(id) {
        var type = "POST";
        var url = "/Mmahasiswa/edit";
        var data = {
            id: id
        };
        $.ajax({
            type: type,
            url: url,
            dataType: "json",
            data: data,
            success: function (res) {
                if (res.code == 200) {
                    for (var i = 0; i < res.data.length; i++) {
                        $('#ve_id').val(res.data[i].id);
                        $('#ve_kode').val(res.data[i].idMahasiswa);
                        $('#ve_nama').val(res.data[i].namaLengkap);
                        $('#ve_JK').val(res.data[i].jenisKelamin);
                        $('#ve_agama').val(res.data[i].agama);
                        $('#ve_tempatlahir').val(res.data[i].tempatLahir);
                        var data = res.data[i].tanggalLahir;
                        var date = new Date(data)
                        yr = date.getFullYear(),
                            mt = date.getMonth() + 1
                        month = mt < 10 ? '0' + mt : mt;
                        dy = date.getDate() < 10 ? '0' + date.getDate() : date.getDate();;
                        var tgl = (yr) + '-' + (month) + '-' + (dy);
                        $('#ve_tgllahir').val(tgl);
                        $('#ve_alamat').val(res.data[i].alamat);
                        $('#ve_telepon').val(res.data[i].telepon);
                        $('#ve_email').val(res.data[i].email);
                        $('#ve_pendidikan').val(res.data[i].pendidikanTerakhir);
                        $('#ve_jurusan').val(res.data[i].jurusan);
                        $('#ve_namasekolah').val(res.data[i].namaSekolah);
                        $('#ve_namaortu').val(res.data[i].namaOrangTua);
                        $('#ve_teleponorangtua').val(res.data[i].teleponOrangTua);
                        $('#ve_pekerjaan').val(res.data[i].pekerjaan);
                        $('#ve_prodi').val(res.data[i].prodi);
                        $('#ve_kls').val(res.data[i].kelas);
                    }
                } else {
                    console.log(res.data)
                }
            }
        })
    }

    function delte(id) {
        swal({
            title: "Are you sure?",
            text: "Once deleted, you will not be able to recover this imaginary file!",
            icon: "warning",
            buttons: true,
            dangerMode: true,
        }).then(function (willDelete) {
            if (willDelete) {
                var type = "POST";
                var url = "/Mmahasiswa/delete";
                var data = {
                    id: id
                };
                $.ajax({
                    type: type,
                    url: url,
                    data: data,
                    success: function (res) {
                        if (res.code == 200) {
                            swal({
                                title: "Success!",
                                text: res.message,
                                icon: "success",
                                button: "Close!",
                            }).then(function () {
                                $('#datatables').DataTable().ajax.reload();
                            });
                        } else {
                            swal({
                                title: "Server Error!",
                                text: res.message,
                                icon: "error",
                                button: "Close!",
                            });
                        }
                    }
                })
            } else {
                swal({
                    title: "Success!",
                    text: "Data Gagal Dihapus!",
                    icon: "success",
                    button: "Close!",
                });
            }
        })
    }

    jQuery(document).ready(function () {
        dataTablesMhs.init();
    });
</script>