                    type: "post",
                    dataType: "json",
                    data: {
                        id: "<%= idMhs %>"
                    },
                    success: function (res) {
                        if (res.code === 200) {
                            for (var i = 0, j = res.data.length; i < j; i++) {
                                if (res.data[i].status === 8) {
                                    $(".invoice").show()
                                } else if (res.data[i].status === 7) {
                                    $(".card").show()
                                } else {
                                    $(".callout").show()
                                }
