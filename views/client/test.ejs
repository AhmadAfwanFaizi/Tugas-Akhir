                        type: "post",
                        dataType: "json",
                        data: {id: "<%= idMhs %>"}
                    },
                    columns: [
                        {
                            data: "namaLengkap"
                        },
                        {
                            data: "hasil"
                        },
                        {
                            data: "idTes",
                            render: function(id, display, row) {
                                if (row.hasil == 'Fisik Kinesteti') {
                                    return "TEKNIK ALAT BERAT, TEKNIK OTOMOTIF, TEKNIK ELEKTRONIKA, TEKNIK INDUSTRI, BUSINESS ADMINISTRATION, SECERTARY, OFFICE MANAGEMENT, TOUR & TRAVEL, HOTEL"
