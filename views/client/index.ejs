        function daftar() {
            var type = "POST";
            var url = "/client";
            var data = $("#formPendaftaran").serialize();
            $.ajax({
                type: type,
                url: url,
                data: data,
                dataType: "json",
                success: function (res) {
                    if (res.code == 200) {
                        swal({
                            title: "Success!",
                            text: res.message,
                            icon: "success",
                            button: "Close"
                        }).then(function () {
                            $("#formPendaftaran")[0].reset();
                            $('#modalPendaftaran').modal('hide');
                        })
                    } else {
                        swal({
                            title: "Server Eror!",
                            text: res.message,
                            icon: "error",
                            button: "Close"
                        }).then(function () {
                            $("#formPendaftaran")[0].reset();
                            $('#modalPendaftaran').modal('hide');
                        })
                    }
                }
            })
        }
